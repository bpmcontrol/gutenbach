foo\
 data/cmd-hello		data/acl-nonexistent \

# This line is not continued
test bar data/cmd-hello logmask=4 \
data/acl-nonexistent \
\
   \
data/acl-no-such-file
test baz data/cmd-hello logmask=4,5,7 ANYUSER

